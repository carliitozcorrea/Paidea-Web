{% extends 'AdminBundle::base.html.twig' %}
{% block body %}
    <div class="row">
        <div class="col-md-12">
            <div class="panel panel-info">
                <div class="panel-heading">
                    <h3>
                        {% block paneltitle %}
                    {% endblock %}
                    </h3>
                </div>
                <div class="panel-body">
                    {% block content %}
                    {% endblock %}
                </div>
                <div class="panel-footer">
                    {% block panelfooter %}
                    {% endblock %}
                </div>
            </div>
        </div>
    </div>
{% endblock %}
{% block javascripts %}
    <script src="{{ asset('assets/js/admin_forms.js') }}"></script>
{% endblock %}