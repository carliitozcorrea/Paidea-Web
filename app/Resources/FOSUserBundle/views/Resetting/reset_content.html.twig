<div class="col-md-6 col-sm-6 col-md-offset-3 col-sm-offset-3" style="margin-top: 30px;">
    <div class="panel panel-primary">
        <div class="panel-heading">
            <h3>{{ 'login.reset.password'|trans }}</h3>
        </div>
        <div class="panel-body">    
            <form action="{{ path('fos_user_resetting_reset', {'token': token}) }}" {{ form_enctype(form) }} method="POST" class="fos_user_resetting_reset">
                {{ form_widget(form) }}
                <div>
                    <button class="btn btn-primary" type="submit">{{ 'resetting.reset.submit'|trans({}, 'FOSUserBundle') }}</button>
                </div>
            </form>
        </div>
    </div>
</div>
