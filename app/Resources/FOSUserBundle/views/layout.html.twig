{% extends 'base.html.twig' %}
{% block title %}
    {{ parent() }}
    Layout
{% endblock %}
{% block body %}
    <div class="header">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <!-- Logo -->
                    <div class="logo">
                        <h1><a href="{{ path('fos_user_security_login') }}">T Layout</a></h1>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="page-content container">
        <div class="row">
            {% for type, messages in app.session.flashBag.all %}
                {% for message in messages %}
                    <div class="{{ type }}">
                        {{ message|trans({}, 'FOSUserBundle') }}
                    </div>
                {% endfor %}
            {% endfor %}
            {% block fos_user_content %}
            {% endblock fos_user_content %}
        </div>
    </div>
   
{% endblock %} 